<header>
  <div class="container">
      <img src="../../images/logo.png" id="logo" alt="FrankenKopter Logo">
  </div>
  <nav class="nav-admin">
    <ul>
      <li><a href="/admin">Home</a></li>
      <li><a class="active" href="/admin/emails">Emails</a></li>
      <li><a href="/admin/testimonials">Testimonials</a></li>
      <li><a href="/logout">Log Out</a></li>
    </ul>
  </nav>
</header>
<main class="emails">
    <% @emails.each do |email| %>
      <ul>
        <li><%= email[:first_name].capitalize %></li>
        <li><%= email[:last_name].capitalize %></li>
        <li><%= email[:email] %></li>
        <li><%= email[:sent] %></li>
        <li><%= email[:viewed] == 'f' ? false : true %></li>
        <li><%= email %></li>
      </ul>
    <% end %>
</main>
